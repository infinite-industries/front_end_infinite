{
  "name": "my-glitch-app",
  "version": "0.0.1",
  "description": "Prototype art event portal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start-dev": "concurrently \"npm run watchify\" \"nodemon ./server.js\"",
    "watchify": "watchify -vd -p browserify -d -e ./src/main.js -o ./public/js/dist.js",
    "production-build": "NODE_ENV=production browserify -g envify -e ./src/main.js | uglifyjs -c -m > ./public/js/dist.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "dependencies": {
    "addtocalendar": "^1.5.3",
    "async": "^2.6.0",
    "aws-sdk": "^2.217.1",
    "axios": "^0.16.2",
    "body-parser": "^1.18.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "ics": "^2.4.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "multiparty": "^4.1.3",
    "nunjucks": "^3.0.1",
    "request": "^2.85.0",
    "sanitizer": "^0.1.3",
    "slack-notify": "^0.1.6",
    "uuid": "^3.2.1",
    "vue": "^2.4.4",
    "vue-router": "^3.0.1",
    "vue2-editor": "^2.4.2",
    "vuetify": "^0.15.7"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "concurrently": "^3.5.1",
    "envify": "^4.1.0",
    "nodemon": "^1.17.2",
    "uglify": "^0.1.5",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "repository": {
    "url": "https://github.com/infinite-industries/front_end_infinite"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "express",
    "art",
    "culture",
    "open source"
  ]
}
